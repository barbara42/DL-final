	author = {Wang, Tongzhou and Isola, Phillip},
	month = aug,
	year = {2022},
	note = {arXiv:2005.10242 [cs, stat]},
	keywords = {Computer Science - Machine Learning, Computer Science - Computer Vision and Pattern Recognition, Statistics - Machine Learning},
	file = {arXiv.org Snapshot:/Users/birdy/Zotero/storage/WFW3P94A/2005.html:text/html;Full Text PDF:/Users/birdy/Zotero/storage/KJZGVSLC/Wang and Isola - 2022 - Understanding Contrastive Representation Learning .pdf:application/pdf},
}

@inproceedings{chen_atom_2021,
	address = {Cham},
	series = {Lecture {Notes} in {Computer} {Science}},
	title = {{ATOM}: {Robustifying} {Out}-of-{Distribution} {Detection} {Using} {Outlier} {Mining}},
