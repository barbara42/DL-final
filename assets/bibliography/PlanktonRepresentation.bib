	pages = {1100--1111},
	file = {arXiv.org Snapshot:/Users/birdy/Zotero/storage/IB8D8VD8/2104.html:text/html;Full Text PDF:/Users/birdy/Zotero/storage/XFIETIDR/Zhou et al. - 2021 - Contrastive Out-of-Distribution Detection for Pret.pdf:application/pdf},
}

