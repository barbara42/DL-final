	keywords = {CNN, Dinophysis ovum, Gulf of Mexico, HAB, Imaging FlowCytobot, Karenia brevis, Prorocentrum texanum},
	pages = {28544--28555},
	file = {Full Text PDF:/Users/birdy/Zotero/storage/HPYZHY65/Henrichs et al. - 2021 - Application of a convolutional neural network to i.pdf:application/pdf},
}

